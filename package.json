    "request": "^2.53.0"
  },
  "devDependencies": { 
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "debug": "^2.2.0",
    "eslint": "^2.5.3",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel --presets es2015 -d lib/node src --source-maps inline",
