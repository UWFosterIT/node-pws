{
  "name": "uwpws",
  "version": "3.0.0",
  "description": "Client Library for University of Washington's Person Web Service",
  "bugs:": {
    "url": "https://github.com/UWFosterIT/node-pws/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UWFosterIT/node-pws/"
  },
  "author": "Marc Miles",
  "main": "./lib/node/index.js",
  "keywords": [
    "uw",
    "person",
    "web",
    "service",
    "pws"
  ],
  "dependencies": {
    "@types/node": "^7.0.8",
    "@types/query-string": "^3.0.32",
    "@types/request": "0.0.41",
    "@types/winston": "^2.2.0",
    "micro-cache": "^2.3.0",
    "query-string": "^4.1.0",
    "request": "^2.72.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@types/babel-core": "^6.7.14",
    "@types/chai": "^3.4.35",
    "@types/debug": "0.0.29",
    "@types/mocha": "^2.2.40",
    "@types/sinon": "^1.16.36",
    "@types/sinon-chai": "^2.7.27",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "debug": "^2.2.0",
    "eslint": "^3.18.0",
    "mocha": "^3.2.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel --presets es2015 -d lib/node src --source-maps inline",
    "lint": "eslint test/**/*.js src/**/*.js",
    "test": "npm run build && ./node_modules/.bin/mocha --bail --reporter spec --compilers js:babel-register --require babel-polyfill test/unit/*.js",
    "debug": "LOG_LEVEL=debug npm run build && ./node_modules/.bin/mocha --debug-brk --bail --reporter dot --compilers js:babel-register test/unit/*.js",
    "bumpver": "npm version --no-git-tag-version patch && git commit package.json -m 'Bumping version.'"
  },
  "engines": {
    "node": ">= v6.9.1"
  },
  "license": "MIT"
}
